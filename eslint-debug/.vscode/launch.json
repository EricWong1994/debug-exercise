{
	"configurations": [
		{
			"name": "Launch Program",
			"program": "${workspaceFolder}/node_modules/.bin/eslint",
			"args": ["./index.js", "--fix"],
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"console": "integratedTerminal",
			"type": "node"
		},
		{
			"name": "调试 eslint api",
			"program": "${workspaceFolder}/api-test.js",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"console": "integratedTerminal",
			"type": "pwa-node"
		}
	]
}
